{
  "_version": "1.81.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.1",
      "toolsId": "2ad73bb2-2c67-47f8-adbf-a06c3b217e70"
    },
    "dataSources": {
      "ODataV2": {
        "uri": "/odata/v2/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/oDatav2/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.144.1",
      "libs": {
        "sap.f": {},
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      },
      "v2": {
        "dataSource": "ODataV2",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "controlAggregation": "beginColumnPages",
        "controlId": "layout",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "project1.view",
        "async": true,
        "viewPath": "project1.view"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": ["TargetMain"]
        },
        {
          "name": "RouteCreate",
          "pattern": "create",
          "target": ["TargetMain", "TargetCreate"]
        },
        {
          "name": "RouteDetail",
          "pattern": "Products/{productId}/:?query:",
          "target": ["TargetMain", "TargetDetail"]
        }
      ],
      "targets": {
        "TargetMain": {
          "name": "Main",
          "id": "Main",
          "level": 1
        },
        "TargetCreate": {
          "name": "ObjectCreate",
          "id": "Create",
          "level": 1,
          "controlAggregation": "midColumnPages"
        },
        "TargetDetail": {
          "name": "ObjectDetail",
          "id": "Detail",
          "level": 1,
          "controlAggregation": "midColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "project1.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
